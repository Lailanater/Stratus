{
  "name": "vxmlgenerator_xp",
  "version": "0.1.0",
  "main": "public/electron.js",
  "repository": "https://github.com/Lailanater/VXMLGenerator_XP.git",
  "author": "Chris Lail <chrislail56@gmail.com>",
  "homepage": "./",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "bootstrap": "^4.3.1",
    "cross-env": "^6.0.3",
    "electron-is-dev": "^1.1.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-scripts": "3.1.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "concurrently \"cross-env BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electron .\"",
    "prepackage": "yarn build",
    "package": "electron-builder build -c.extraMetadata.main=build/electron.js"
  },
  "build": {
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^5.0.0",
    "electron": "^6.0.12",
    "electron-builder": "^21.2.0",
    "wait-on": "^3.3.0"
  }
}
